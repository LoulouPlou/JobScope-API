{
  "server": {
    "http": {
      "enabled": true,
      "port": 3000
    },
    "https": {
      "enabled": true,
      "port": 3443,
      "keyPath": "./ssl/key.pem",
      "certPath": "./ssl/cert.pem",
      "redirectAllHttpToHttps": false
    }
  },
  "security": {
    "cors": {
      "origin": ["http://localhost:3000", "https://localhost:3443"],
      "credentials": true
    },
    "rateLimit": {
      "windowMs": 900000,
      "max": 1000
    }
  }
}